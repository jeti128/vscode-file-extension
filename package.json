{
    "name": "vscode-file-extension",
    "displayName": "vsCode file extension",
    "description": "VSCode extension for file mannage",
    "version": "1.0.0",
    "publisher": "jeti128",
    "keywords": [
        "vscode",
        " file mannage",
        "newfile"
    ],
    "icon": "https://github.com/jeti128/vscode-file-extension/blob/master/assets/icons/vscode_fileExtension.png",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.newFile",
        "onCommand:extension.openFile",
        "onCommand:extension.searchFile",
        "onCommand:extension.mainmenu",
        "onCommand:extension.newFolder"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
                "command": "extension.newFile",
                "title": "New file"
            },
            {
                "command": "extension.openFile",
                "title": "Open file"
            },
            {
                "command": "extension.searchFile",
                "title": "Search file"
            },
            {
                "command": "extension.mainmenu",
                "title": "jtFileExtension men√º"
            },
            {
                "command": "extension.newFolder",
                "title": "New folder"
            }
        ],
        "keybindings": [{
                "command": "extension.mainmenu",
                "key": "Alt+F11",
                "mac": "cmd+f11"
            },
            {
                "command": "extension.newFile",
                "key": "Alt+F9",
                "mac": "cmd+f9"
            },
            {
                "command": "extension.openFile",
                "key": "Alt+F10",
                "mac": "cmd+f10"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.41.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jeti128/vscode-file-extension.git"
    },
    "dependencies": {
        "fs-sync": "^1.0.6"
    }
}